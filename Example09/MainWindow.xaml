<Window x:Class="Example09.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Example09"
        mc:Ignorable="d"
        Title="Example 09" Height="450" Width="800">
	<Grid>
        <!--TabControls offer a way to easily and visually classify large UI's with
        multiple sections in smaller segments.
        
        The TabControl in this example is programatically populated, so as to demonstrate
        how WPF controls can be dynamically manipulated based on logical requirements.-->
		<TabControl Name="myTabs"/>
	</Grid>
</Window>
